<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kitri.review">
	<insert id="write" parameterType="review">
		insert into review 
		values(revno_seq.nextval,#{revgrade},#{revcomment},sysdate,#{mem_id},#{revgood},#{revbad},#{prf_id})
	</insert>
	<select id="list" resultType="review">
		select * from review
	</select>
</mapper>







