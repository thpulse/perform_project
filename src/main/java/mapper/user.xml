     								,#{user_gender}
     								,#{user_gerne})
     								<!-- ,#{user_play}
     								,#{user_musical}
     								,#{user_classic}
     								,#{user_opera}
     								,#{user_dance}
     								,#{user_ballet}
     								,#{user_gugak})   -->
    </insert>

</mapper>