<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kitri.perform.user">

	<insert id="insert" parameterType="insert">
     	insert into userinfo values(#{user_id}
     								,#{user_pass}
